{
  "name": "angular-schema-form-bootstrap",
  "version": "1.0.0-alpha.1",
  "description": "Bootstrap 3 decorator for Angular Schema Form",
  "main": "dist/bootstrap-decorator.js",
  "scripts": {
    "dist": "npm run build && npm run minify",
    "build": "webpack",
    "watch": "webpack --watch",
    "minify": "uglifyjs dist/angular-schema-form-bootstrap.js --comments --output=dist/angular-schema-form-bootstrap.min.js && uglifyjs dist/angular-schema-form-bootstrap-bundled.js --comments --output=dist/angular-schema-form-bootstrap-bundled.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-not-yet-done": "karma start --single-run --browsers PhantomJS karma.conf.js --log-level info"
  },
  "keywords": [
    "angular-schema-form-decorator"
  ],
  "author": "json-schema-form",
  "contributors": [
    "David Jensen <david.lgj@gmail.com> (https://github.com/davidlgj)",
    "Marcel J Bennett <iamanthropic@gmail.com> (https://github.com/Anthropic)"
  ],
  "license": "MIT",
  "dependencies": {
    "angular": ">= 1.2",
    "angular-messages": "^1.5.0",
    "angular-sanitize": ">= 1.2",
    "angular-schema-form": "git://github.com/json-schema-form/angular-schema-form.git#feature/webpack-babel",
    "tv4": "~1.0.15"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.0",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.6.0",
    "gulp-concat": "^2.5.2",
    "gulp-jscs": "^3.0.2",
    "gulp-minify-html": "^1.0.2",
    "gulp-protractor": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "gulp-watch": "^4.2.4",
    "gulp-webserver": "^0.9.1",
    "html": "^1.0.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "karma": "^0.13.22",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.0.0",
    "karma-growler-reporter": "0.0.1",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "0.0.1",
    "ng-cache-loader": "0.0.16",
    "ngtemplate-loader": "^1.3.1",
    "protractor": "^2.5.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "streamqueue": "^0.1.3",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
